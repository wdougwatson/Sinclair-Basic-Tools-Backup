  50 REM rainbow
 100 BORDER 0: INK 7: PAPER 0: CLS : RESTORE 1000: FOR m=0 TO 7
 110 READ a,b,c: FOR n=0 TO 11*(m<7): PLOT PAPER b; INK c; INVERSE a;0,n+12*m: DRAW PAPER b; INK c; INVERSE a;255,0,-1.5: IF ABS ((m/2)-INT (m/2))>.1 THEN LET n=n+7
 120 NEXT n: NEXT m
 130 PAPER 0: INK 7: INVERSE 0
 140 STOP
1000 DATA 0,0,2,1,6,2,0,6,4,1,5,4,0,5,1,1,3,1,0,3,0,1,0,0
